apiVersion: extensions/v1beta1
kind: Deployment
metadata:
  name: php-56
  labels:
    app: php-56
spec:
  replicas: 1
  selector:
    matchLabels:
      app: php-56
  template:
    metadata:
      labels:
        app: php-56
    spec:
      containers:
      - name: php-56
        image: 172.16.251.116:5000/php5.6:v1.0
        ports:
        - containerPort: 9000
        volumeMounts:
        - mountPath: /www
          readOnly: false
          name: php-56
      volumes:
      - name: php-56
        nfs:
          server: 192.168.159.129
          path: "/www"
